class Solution {
public:
    int maxEle(vector<int>&nums){
        int maxi = INT_MIN;
        int ele = nums[0];
        for(int i =1;i<nums.size();i++){
            if(ele<nums[i]){
                ele = nums[i];
            }
        }
        return ele;
    }
    int sumOfDiv(vector<int>&nums,int div){
        int sum = 0;

        for(int i =0;i<nums.size();i++){
            sum += ceil(double(nums[i])/double(div));
        }
        return sum;
    }
    int smallestDivisor(vector<int>& nums, int threshold) {
        int low = 1;
        int high = maxEle(nums);
        int ans = -1;
        while(low<=high){
            int mid = (low+high)/2;

            if(sumOfDiv(nums,mid)<=threshold){
                 ans = mid;
                 high = mid-1;
            }else{
                low = mid+1;
            }
        }
        return ans;
    }
};
