class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        string ans = "";

        for(int i = 0;i<strs[0].length();i++){
            char ch = strs[0][i];
            cout<<ch<<endl;
            bool isall = true;
            for(int j = 1;j<strs.size();j++){
                if(strs[j].size()<i || ch!=strs[j][i]){
                    isall = false;
                    break;
                }
            }
            if(isall ==false){
                break;
            }else{
                ans.push_back(ch);
            }
        }
        return ans;
    }
};
